<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block pagetitle %}Net Gusto's{% endblock %}</title>

    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>

    {% block basestylesheets %}
    <!-- bootstrap 3.0.2 -->
    <link href="{{ asset('css/lib/app.bootstrap.css') }}" rel="stylesheet" type="text/css" />
    <!-- font Awesome -->
    <link href="{{ asset('css/lib/font-awesome.css') }}" type="text/css" rel="stylesheet" />
    <!-- Ionicons -->
    <link href="{{ asset('vendor/almasaeed2010/adminlte/css/ionicons.min.css') }}" rel="stylesheet" type="text/css" />
    <!-- Theme style -->
    <link href="{{ asset('css/lib/admin.adminlte.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}

    {% block stylesheets %}{% endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
</head>
<body class="{% block bodyclass %}skin-blue{% endblock %}">

    <!-- header logo: style can be found in header.less -->
    <header class="header">
        
        {% block brand %}<a href="#" class="logo">Net Gusto's</a>{% endblock %}

        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
        
            <!-- Sidebar toggle button-->
            <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </a>
            <div class="navbar-right">
                {% block navbar %}{% endblock %}
            </div>
        </nav>
    </header>

    <div class="wrapper row-offcanvas row-offcanvas-left">

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="left-side sidebar-offcanvas">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">

                {% block leftmenu %}{{ adminlte_leftmenu_render('NetgustoAdminLTEBundle:DemoMenu:left') }}{% endblock %}

            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Right side column. Contains the navbar and content of the page -->
        <aside class="right-side">

            <!-- Main content -->
            <section class="content">
                {% block body %}{% endblock %}
            </section><!-- /.content -->

        </aside><!-- /.right-side -->
    </div><!-- ./wrapper -->

    {% block basejavascripts %}
        {% include 'NetgustoAdminLTEBundle:Page/_partials:basejavascripts.html.twig' %}
    {% endblock %}
    
    {% block javascripts %}
        <script type="text/javascript">require(['admin/main']);</script>
    {% endblock %}
</body>
</html>